{
   "version": 2,
   "routes": [
     {
       "src": "/api/(.*)",
       "dest": "index.js"
     },
     {
       "src": "/(.*)",
       "dest": "public/$1"
     }
   ],
   "env": {
     "NODE_ENV": "production"
   },
   "builds": [
     {
       "src": "index.js",
       "use": "@vercel/node"
     }
   ]
 }
 